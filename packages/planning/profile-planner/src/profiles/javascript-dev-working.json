{
  "name": "javascript-dev-working",
  "version": "1.0.0",
  "description": "Working JavaScript development profile with actually available tools",
  "toolName": "javascript_dev_working_planner",
  "verified": true,
  "allowableActions": [
    {
      "type": "file_write",
      "description": "Write content to a file",
      "inputs": {
        "filepath": {
          "type": "string",
          "description": "Path where the file should be created"
        },
        "content": {
          "type": "string",
          "description": "Content to write to the file"
        }
      },
      "outputs": {
        "file_created": {
          "type": "string",
          "description": "Path to the created file"
        }
      }
    },
    {
      "type": "create_file",
      "description": "Create a file (alternative to file_write)",
      "inputs": {
        "filepath": {
          "type": "string",
          "description": "Path of the file to create"
        },
        "content": {
          "type": "string",
          "description": "Content for the file"
        }
      },
      "outputs": {
        "file_created": {
          "type": "string",
          "description": "Path to the created file"
        }
      }
    },
    {
      "type": "run_command",
      "description": "Execute a command in the shell",
      "inputs": {
        "command": {
          "type": "string",
          "description": "Command to execute (e.g., 'node test.js', 'npm init -y')"
        }
      },
      "outputs": {
        "stdout": {
          "type": "string",
          "description": "Standard output from the command"
        },
        "success": {
          "type": "boolean",
          "description": "True if command succeeded"
        }
      }
    },
    {
      "type": "node_run_command",
      "description": "Run a Node.js command specifically",
      "inputs": {
        "command": {
          "type": "string",
          "description": "Node.js command to execute"
        }
      },
      "outputs": {
        "stdout": {
          "type": "string",
          "description": "Standard output from the command"
        }
      }
    }
  ],
  "defaultInputs": [
    "user_request"
  ],
  "defaultOutputs": [
    "file_created",
    "stdout"
  ],
  "maxSteps": 8,
  "contextPrompts": [
    "You are creating a JavaScript project using the available tools.",
    "Use file_write or create_file to create files with content.",
    "Use run_command or node_run_command to execute shell commands.",
    "Create simple, working Node.js code.",
    "Test your code by running it with Node.js."
  ]
}